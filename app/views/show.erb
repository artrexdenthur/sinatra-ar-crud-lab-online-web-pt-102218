<h1>View Post <%=@article.id%></h1>
<h2><%=@article.title%></h2>
<p><%=@article.content%></p>
<form action="/articles/<%= @article.id %>" method="post">
  <input type="submit" name="delete" value="Delete" />
</form>
<form action="/articles/" method="get">
  <input type="submit" name="to_index" value="Back to Index" />
</form>
